    ::= { pureNetworkPerformance 3 }
 
pureDroppedErrors OBJECT-TYPE
    SYNTAX OCTET STRING
    UNITS  "Errors/s"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Array transmitted packets dropped per second."
    ::= { pureNetworkPerformance 4 }
 
pureOtherErrors OBJECT-TYPE
    SYNTAX OCTET STRING
    UNITS  "Errors/s"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Array other Errors per second"
    ::= { pureNetworkPerformance 5 }
 
pureTotalErrors OBJECT-TYPE
    SYNTAX OCTET STRING
    UNITS  "Errors/s"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The sum of all reception and transmission errors per second."
    ::= { pureNetworkPerformance 6 }

pureReceivedBytes OBJECT-TYPE
    SYNTAX OCTET STRING
    UNITS  "Bytes/s"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Bytes received per second."
    ::= { pureNetworkPerformance 7 }

pureTransmittedBytes OBJECT-TYPE
    SYNTAX OCTET STRING
    UNITS  "Bytes/s"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Bytes transmitted per second."
    ::= { pureNetworkPerformance 8 }

pureReceivedPackets OBJECT-TYPE
    SYNTAX OCTET STRING
    UNITS  "Packets/s"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Packets received per second."
    ::= { pureNetworkPerformance 9 }

pureTransmittedPackets OBJECT-TYPE
    SYNTAX OCTET STRING
    UNITS  "Packets/s"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Packets transmitted per second."
    ::= { pureNetworkPerformance 10 }

