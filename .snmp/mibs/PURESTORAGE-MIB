PURESTORAGE-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY,
    OBJECT-IDENTITY,
    NOTIFICATION-TYPE,
    OBJECT-TYPE,
    Integer32,
    enterprises
        FROM SNMPv2-SMI
    OBJECT-GROUP
        FROM SNMPv2-CONF;

purestorage MODULE-IDENTITY
    LAST-UPDATED "202401010000Z"
    ORGANIZATION "Pure Storage, Inc"
    CONTACT-INFO
        "http://www.purestorage.com/ | support@purestorage.com"
    DESCRIPTION
        "Pure Storage v2c/v3 SNMP MIB"
    REVISION "202401010000Z"
    DESCRIPTION
        "Pure Storage Flash Blade MIB extended with data on Array identification, hardware and networking."
    REVISION "201905090000Z"
    DESCRIPTION
        "Pure Storage Flash Blade MIB."
    ::= { enterprises 40482 }

pureSystem OBJECT-IDENTITY
    STATUS  current
    DESCRIPTION
        "Parent of all managed objects relating to system identification."
    ::= { purestorage 1 }

pureNotifications OBJECT-IDENTITY
    STATUS  current
    DESCRIPTION
        "Parent of all notifications (traps, informs)."
    ::= { purestorage 2 }

pureObjects OBJECT-IDENTITY
    STATUS  current
    DESCRIPTION
        "Parent of all objects."
    ::= { purestorage 3 }

purePerformance OBJECT-IDENTITY
    STATUS  current
    DESCRIPTION
        "Parent of all performance data."
    ::= { purestorage 4 }

pureHardware OBJECT-IDENTITY
    STATUS  current
    DESCRIPTION
        "Parent of all hardware data."
    ::= { purestorage 5 }
 
pureNetwork OBJECT-IDENTITY
    STATUS  current
    DESCRIPTION
        "Parent of all networking data."
    ::= { purestorage 6 }
 
pureNetworkPerformance OBJECT-IDENTITY
    STATUS  current
    DESCRIPTION
        "Parent of all network performance data."
    ::= { purestorage 7 }

pureExperimental OBJECT-IDENTITY
    STATUS  current
    DESCRIPTION
        "Used for product testing and development."
    ::= { purestorage 700 }

pureProductName OBJECT-TYPE
    SYNTAX OCTET STRING
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Pure Storage Product Name."
    ::= { pureObjects 1 }

pureProductVersion OBJECT-TYPE
    SYNTAX OCTET STRING
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Pure Storage Product Version."
    ::= { pureObjects 2 }

